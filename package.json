    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "vue-cli-plugin-svg": "^0.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "presets": [
